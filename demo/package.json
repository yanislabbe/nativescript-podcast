{
  "dependencies": {
    "@nativescript/core": "~8.8.2",
    "nativescript-podcast": "file:../src",
    "nativescript-theme-core": "^1.0.7"
  },
  "devDependencies": {
    "@nativescript/android": "8.8.5",
    "@nativescript/ios": "8.8.2",
    "@nativescript/types": "~8.8.0",
    "@nativescript/webpack": "~5.0.22",
    "tslint": "~6.1.3",
    "typescript": "~5.6.3"
  },
  "scripts": {
    "build.plugin": "cd ../src && npm run build",
    "ci.tslint": "npm i && tslint --config '../tslint.json' 'app/**/*.ts' --exclude '**/node_modules/**' --exclude '**/typings/**'",
    "ci.android.build": "npm run build.plugin && tns build android",
    "ci.ios.build": "npm run build.plugin && tns build ios",
    "e2e": "tsc -p e2e && mocha --opts ./e2e/config/mocha.opts",
    "compile-tests": "tsc -p e2e --watch"
  },
  "main": "app.js"
}
