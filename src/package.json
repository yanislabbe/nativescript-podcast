{
  "name": "nativescript-podcast",
  "version": "1.0.5",
  "description": "NativeScript plugin to play audio.",
  "main": "audio",
  "typings": "index.d.ts",
  "nativescript": {
    "platforms": {
      "android": "8.8.5",
      "ios": "8.8.2"
    }
  },
  "scripts": {
    "tsc": "tsc -skipLibCheck",
    "build": "npm i && ts-patch install && tsc",
    "tslint": "cd .. && tslint \"**/*.ts\" --config tslint.json --exclude \"**/node_modules/**\" --exclude \"*demo*/platforms/**\"",
    "ci.tslint": "npm i && tslint '**/*.ts' --config '../tslint.json' --exclude '**/node_modules/**' --exclude '**/typings/**'",
    "demo.ios": "npm run build && cd ../demo && ns run ios",
    "demo.android": "npm run build && cd ../demo && ns run android",
    "clean.src": "rimraf **/*.js **/*.js.map node_modules package-lock.json",
    "clean.demo": "cd ../demo && ns clean",
    "generate.typings.ios": "cd ../demo && TNS_DEBUG_METADATA_PATH=\"$(pwd)/metadata\" ns build ios && TNS_TYPESCRIPT_DECLARATIONS_PATH=\"$(pwd)/typings\" ns build ios && echo 'Now look for your library typings in demo/typings!'"
  },
  "lint-staged": {
    "**/*.{js, css, ts, json, scss, html, xml, md}": [
      "prettier --write",
      "git add"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "pre-push": "npm run tslint"
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/yanislabbe/nativescript-podcast.git"
  },
  "keywords": [
    "NativeScript",
    "JavaScript",
    "TypeScript",
    "Android",
    "iOS",
    "music",
    "audio",
    "podcast"
  ],
  "author": {
    "name": "Yanis Labbe",
    "url": "https://github.com/yanislabbe"
  },
  "bugs": {
    "url": "https://github.com/yanislabbe/nativescript-podcast/issues"
  },
  "license": "MIT",
  "homepage": "https://github.com/yanislabbe/nativescript-podcast",
  "readmeFilename": "README.md",
  "devDependencies": {
    "@nativescript/core": "^8.8.2",
    "@nativescript/types": "^8.8.0",
    "@nativescript/webpack": "~5.0.22",
    "husky": "~9.1.6",
    "lint-staged": "~15.2.10",
    "prettier": "^3.3.3",
    "rimraf": "^6.0.1",
    "ts-node": "^10.9.2",
    "ts-patch": "^3.2.1",
    "tslint": "~6.1.3",
    "typescript": "~5.6.3"
  }
}
